{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Products"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "category": {
      "type": "enumeration",
      "enum": [
        "NISSAN",
        "UD_TRUCKS",
        "UNICARRIER",
        "EICHER",
        "MAC_POWER"
      ],
      "unique": false
    },
    "name": {
      "type": "string"
    },
    "motto": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "headerImg": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "specification": {
      "type": "component",
      "repeatable": true,
      "component": "specification.specification"
    },
    "link": {
      "type": "string"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "brochure": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "eachCategory": {
      "type": "enumeration",
      "enum": [
        "PASSENGER",
        "CROSSOVER",
        "SPORT_UTILITY",
        "COMMERCIAL",
        "CRONER",
        "QUESTER",
        "NEW_QUESTER",
        "EICHER_SKYLINE_BUS",
        "EICHER_PRO_3008",
        "MACPOWER"
      ]
    },
    "vrImg": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
