    "@graphql-codegen/typescript-react-apollo": "2.0.6",
    "@loadable/component": "5.13.1",
    "aframe": "1.0.4",
    "aframe-particle-system-component": "1.1.3",
    "aframe-react": "4.4.0",
    "apollo-boost": "0.4.9",
    "dotenv": "8.2.0",
    "dotenv-cli": "3.2.0",
    "find-config": "1.0.0",
    "gatsby": "2.24.55",
    "gatsby-image": "2.4.17",
    "gatsby-plugin-google-tagmanager": "2.3.11",
    "gatsby-plugin-intl": "0.3.3",
