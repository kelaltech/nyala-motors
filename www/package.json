{
  "name": "nyala-motors-www",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "run-p": "run-p",
    "run-s": "run-s",
    "gatsby": "gatsby",
    "gatsby-ast-only": "graphql codegen --project gatsby-ast",
    "gatsby-ast": "dotenv run-s gatsby-ast-only",
    "schema": "dotenv run-p gatsby-ast",
    "codegen": "dotenv graphql codegen",
    "gen-only": "graphql codegen --watch",
    "gen": "dotenv run-s gen-only",
    "clean": "gatsby clean",
    "type-check": "tsc --pretty --noEmit",
    "ts": "run-s \"type-check --watch --preserveWatchOutput\"",
    "build-only": "gatsby build",
    "build": "cross-env NODE_ENV=production dotenv run-s codegen build-only type-check",
    "prod": "run-p format \"run-s clean build\"",
    "serve": "cross-env NODE_ENV=production dotenv gatsby serve",
    "develop": "cross-env NODE_ENV=development dotenv run-s codegen && run-p gen \"gatsby develop --open\" ts",
    "dev": "run-s develop",
    "start": "run-s clean dev",
    "lint": "prettier src gatsby-*.js",
    "format": "prettier --write src gatsby-*.js",
    "test": "jest --passWithNoTests"
  },
  "resolutions": {
    "@types/node": "^12",
    "@types/styled-components": "^5",
    "styled-components": "^5"
  },
  "dependencies": {
    "@apollo/client": "3.1.4",
